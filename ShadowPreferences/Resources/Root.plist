		<!-- File Map -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>File Map</string>
			<key>footerText</key>
			<string>File map enhances Shadow's ability to perform path checks by scanning all installed packages. The file map will need to be regenerated when any changes to installed packages are made. Live generation will generate the file map upon application launch, which may increase launch times.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>action</key>
			<string>generate_map:</string>
			<key>label</key>
			<string>Generate File Map</string>
			<key>confirmation</key>
			<dict>
				<key>prompt</key>
				<string>This will read all installed package files. This may take a while, depending on how many packages are installed.</string>
				<key>title</key>
				<string>Continue</string>
				<key>okTitle</key>
				<string>Continue</string>
				<key>cancelTitle</key>
				<string>Cancel</string>
			</dict>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>me.jjolano.shadow</string>
			<key>key</key>
			<string>auto_file_map_generation_enabled</string>
			<key>label</key>
			<string>Live Generation</string>
		</dict>

		<!-- Dynamic Libraries -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Dynamic Libraries</string>
			<key>footerText</key>
			<string>Filtering dynamic libraries helps defeat intermediate levels of jailbreak detection. Advanced filters may trigger detection on some apps, and may also be unstable.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>me.jjolano.shadow</string>
			<key>key</key>
			<string>dyld_hooks_enabled</string>
			<key>label</key>
			<string>Enable Filtering</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>me.jjolano.shadow</string>
			<key>key</key>
			<string>dyld_filter_enabled</string>
			<key>label</key>
			<string>Enable Advanced Filters</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Dynamic Linker</string>
			<key>footerText</key>
			<string>Advanced jailbreak detection bypass. May be unstable in some apps, causing crashes or other issues.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>bundle</key>
			<string>AppList</string>
			<key>isController</key>
			<string>1</string>
			<key>label</key>
			<string>Filter Dynamic Linker</string>
			<key>ALAllowsSelection</key>
			<string>0</string>
			<key>ALSettingsKeyPrefix</key>
			<string>dlfcn</string>
			<key>ALSettingsDefaultValue</key>
			<string>0</string>
			<key>ALSettingsPath</key>
			<string>/var/mobile/Library/Preferences/me.jjolano.shadow.plist</string>
			<key>ALSingleEnabledMode</key>
			<false/>
			<key>ALSectionDescriptors</key>
			<array>
				<dict>
					<key>cell-class-name</key>
					<string>ALSwitchCell</string>
					<key>icon-size</key>
					<string>29</string>
					<key>suppress-hidden-apps</key>
					<string>1</string>
					<key>title</key>
					<string>Select apps to enable dynamic linker bypass</string>
					<key>predicate</key>
					<string>isSystemApplication = FALSE</string>
				</dict>
			</array>
		</dict>

		<!-- Sandbox Integrity -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Sandbox Integrity</string>
			<key>footerText</key>
			<string>Attempts to restore original sandbox functionality. Only necessary if apps are able to use methods that are otherwise restricted by the sandbox.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>me.jjolano.shadow</string>
			<key>key</key>
			<string>sandbox_hooks_enabled</string>
			<key>label</key>
			<string>Restore Sandbox</string>
